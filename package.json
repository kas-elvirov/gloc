    "file-loader": "^1.1.11",
    "grunt": "^1.5.3",
    "grunt-cli": "^1.3.2",
    "grunt-contrib-compress": "^1.4.3",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.5",
    "node-json-minify": "^1.0.0",
    "react-iframe": "^1.8.0",
    "ts-loader": "^5.3.3",
    "tslint": "^5.14.0",
    "typescript": "^3.3.4000",
    "typescript-tslint-plugin": "^0.3.1",
    "uglifyjs-webpack-plugin": "^2.1.2",
